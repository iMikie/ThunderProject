<!--<div class="song_page">-->
<div class="song-container debug">
  <div class="song-left_column debug">
    <p class="small_title"> Title: </p>

    <p class="large_field"> <%= @song.title %> </p>

    <% if @song.composer %>
      <p class="small_title"> Composer: </p>

      <p class="large_field"> <%= @song.composer %> </p>
    <% end %>

    <% if @song.voicing %>
      <p class="small_title"> Voicing: </p>

      <p class="large_field"> <%= @song.voicing %> </p>
    <% end %>

    <% if @song.category %>
      <p class="small_title"> Catogory: </p>

      <p class="large_field"> <%= @song.category %> </p>
    <% end %>

    <% if @song.arranger_one %>
      <p class="small_title"> 1st Arranger: </p>

      <p class="large_field"> <%= @song.arranger_one %> </p>
    <% end %>

    <% if @song.arranger_two %>
      <p class="small_title"> 2nd Arranger </p>

      <p class="large_field"> <%= @song.arranger_two %> </p>
    <% end %>
    <%= link_to 'Back', songs_path,  class: "abut abut2 abut3" %>
  </div>
  <div class="song-right_column debug">
    <iframe src="/SOTS_Songs/<%= @song.song_number %>.pdf" class="pdf">


    </iframe>
  </div>
</div>



